<script>

  function about(){
    document.getElementById('about').style.display = 'block';
    document.getElementById('info').style.display = 'none';
    document.getElementById('reviews').style.display = 'none';

    $('.about[data-step="1"]').addClass('current')
    $('.about[data-step="2"]').removeClass('current')
    $('.about[data-step="3"]').removeClass('current')
  }

  function info(){
    document.getElementById('about').style.display = 'none';
    document.getElementById('info').style.display = 'block';
    document.getElementById('reviews').style.display = 'none';


    $('.about[data-step="2"]').addClass('current')
    $('.about[data-step="1"]').removeClass('current')
    $('.about[data-step="3"]').removeClass('current')
  }



  function reviews(){
    document.getElementById('about').style.display = 'none';
    document.getElementById('info').style.display = 'none';
    document.getElementById('reviews').style.display = 'block';


    $('.about[data-step="3"]').addClass('current')
    $('.about[data-step="2"]').removeClass('current')
    $('.about[data-step="1"]').removeClass('current')
  }
</script>








<script>

  function Behavior(){
    document.getElementById('behavior').style.display = 'block';
    document.getElementById('learning').style.display = 'none';
    document.getElementById('infectious').style.display = 'none';
    document.getElementById('congenital').style.display = 'none';

  }

  function Learning(){
    document.getElementById('behavior').style.display = 'none';
    document.getElementById('learning').style.display = 'block';
    document.getElementById('infectious').style.display = 'none';
    document.getElementById('congenital').style.display = 'none';

  }



  function Infectious(){
    document.getElementById('behavior').style.display = 'none';
    document.getElementById('learning').style.display = 'none';
    document.getElementById('congenital').style.display = 'none';
    document.getElementById('infectious').style.display = 'block';
  }


  function Congenital(){
    document.getElementById('behavior').style.display = 'none';
    document.getElementById('learning').style.display = 'none';
    document.getElementById('infectious').style.display = 'none';
    document.getElementById('congenital').style.display = 'block';
  }
</script>





